{
  "name": "soundy",
  "version": "1.0.0",
  "description": "All about the sound",
  "scripts": {
    "build": "vite build",
    "check:full": "npm run typescript && npm run eslint && npm run test",
    "dev": "vite --host",
    "eslint": "eslint .",
    "compile": "graphql-codegen --watch -r dotenv/config",
    "init": "concurrently \"npm run lyrics_library_api\" \"npm run dev\"",
    "server": "cd ../lyrics-library && yarn dev",
    "test": "vitest",
    "typescript": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AnthonyBourret/Soundy.git"
  },
  "contributors": [
    {
      "name": "Anthony Bourret",
      "url": "https://github.com/AnthonyBourret"
    },
    {
      "name": "Guillaume Jolibois",
      "url": "https://github.com/Joliwood",
      "email": "jolibois.contact@gmail.com"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "@apollo/client": "^3.8.8",
    "@graphql-codegen/cli": "^5.0.2",
    "@reduxjs/toolkit": "^2.1.0",
    "@tailwindcss/typography": "^0.5.10",
    "@vitejs/plugin-react": "^4.1.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "daisyui": "^4.4.14",
    "graphql": "^16.8.1",
    "i18next": "^23.7.6",
    "i18next-browser-languagedetector": "^7.2.0",
    "i18next-http-backend": "^2.4.1",
    "postcss": "^8.4.31",
    "react": "^18.2.0",
    "react-audio-player": "^0.17.0",
    "react-cookie": "^7.1.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.4.1",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.18.0",
    "tailwindcss": "^3.3.5",
    "vite": "^4.5.0"
  },
  "devDependencies": {
    "@graphql-codegen/client-preset": "^4.2.4",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/react-redux": "^7.1.33",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
